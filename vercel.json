{
    "version": 2,
    "builds": [
    {
    "src": "MOvie_app/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.9" }
    },
    {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
    "distDir": "staticfiles_build"
     }
    }
    ],
    "routes": [
    {
    "src": "/static/(.*)",
    "dest": "/static/$1"
    },
    {
    "src": "/(.*)",
    "dest": "MOvie_app/wsgi.py"
    }
    ]
    }
